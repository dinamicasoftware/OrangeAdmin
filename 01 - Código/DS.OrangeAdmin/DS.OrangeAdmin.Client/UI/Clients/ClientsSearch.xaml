<ContentControl x:Class="DS.OrangeAdmin.Client.UI.Clients.ClientsSearch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DS.OrangeAdmin.Client.UI.Clients"
        xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
        mc:Ignorable="d"
        Height="312.712" Width="538.347" Background="Red">
    <StackPanel>
        <syncfusion:RibbonBar>
            <syncfusion:RibbonButton SizeForm="Large" Label="Ver Cliente"></syncfusion:RibbonButton>
            <syncfusion:RibbonButton SizeForm="Large" Label="Agregar Cliente"></syncfusion:RibbonButton>
            <syncfusion:RibbonButton SizeForm="Large" Label="Borrar Cliente"></syncfusion:RibbonButton>
            <syncfusion:RibbonButton SizeForm="Large" Label="Ver Cuenta Corriente Cliente"></syncfusion:RibbonButton>
        </syncfusion:RibbonBar>
        <DataGrid Name="grid1" Background="AliceBlue" Margin="0,0,0,0" ItemsSource="{Binding Clients}"/>
    </StackPanel>
</ContentControl>
